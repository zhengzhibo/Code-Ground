    "express-session": "^1.17.1",
    "http-errors": "~1.8.0",
    "lowdb": "^1.0.0",
    "morgan": "~1.10.0",
    "nanoid": "^3.1.10",
    "passport": "^0.4.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
